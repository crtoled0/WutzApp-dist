<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.wutz" version="2.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Wutz</name>
    <description>
      App to connect to Wutz network and choose what you want to hear.
  </description>
    <author email="crtoledo@wutznet.com" href="http://get.wutz.co.uk">
      Cristian Toledo
  </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <plugin name="cordova-plugin-device" source="npm" spec="1.1.6" />
    <plugin name="cordova-plugin-geolocation" source="npm" spec="2.1.0" />
    <plugin name="cordova-plugin-whitelist" source="npm" />
    <plugin name="cordova-plugin-globalization" spec="~1.0.7" />
    <icon src="icon.png" />
    <platform name="android">
        <icon density="ldpi" src="img/logo.png" />
        <icon density="mdpi" src="img/logo.png" />
        <icon density="hdpi" src="img/logo.png" />
        <icon density="xhdpi" src="img/logo.png" />
        <icon density="xxhdpi" src="img/logo.png" />
        <icon density="xxxhdpi" src="img/logo.png" />
        <splash density="land-ldpi" src="res/screen/android/drawable-land-ldpi-screen.png" />
        <splash density="land-mdpi" src="res/screen/android/drawable-land-mdpi-screen.png" />
        <splash density="land-hdpi" src="res/screen/android/drawable-land-hdpi-screen.png" />
        <splash density="land-xhdpi" src="res/screen/android/drawable-land-xhdpi-screen.png" />
        <splash density="land-xxhdpi" src="res/screen/android/drawable-land-xxhdpi-screen.png" />
        <splash density="land-xxxhdpi" src="res/screen/android/drawable-land-xxxhdpi-screen.png" />
        <splash density="port-ldpi" src="res/screen/android/drawable-port-ldpi-screen.png" />
        <splash density="port-mdpi" src="res/screen/android/drawable-port-mdpi-screen.png" />
        <splash density="port-hdpi" src="res/screen/android/drawable-port-hdpi-screen.png" />
        <splash density="port-xhdpi" src="res/screen/android/drawable-port-xhdpi-screen.png" />
        <splash density="port-xxhdpi" src="res/screen/android/drawable-port-xxhdpi-screen.png" />
        <splash density="port-xxxhdpi" src="res/screen/android/drawable-port-xxxhdpi-screen.png" />
    </platform>
    <platform name="ios">
        <!-- iPhone 6 / 6+ -->
        <icon src="icon.png" platform="ios" width="180" height="180" />
        <!-- iPhone / iPod Touch  -->
        <icon src="icon.png" platform="ios" width="60" height="60" />
        <icon src="icon.png" platform="ios" width="120" height="120" />
        <!-- iPad -->
        <icon src="icon.png" platform="ios" width="76" height="76" />
        <icon src="icon.png" platform="ios" width="152" height="152" />
        <icon src="icon.png" platform="ios" width="167" height="167" />
        <!-- Settings Icon -->
        <icon src="icon.png" platform="ios" width="29" height="29" />
        <icon src="icon.png" platform="ios" width="58" height="58" />
        <icon src="icon.png" platform="ios" width="87" height="87" />
        <!-- Spotlight Icon -->
        <icon src="icon.png" platform="ios" width="40" height="40" />
        <icon src="icon.png" platform="ios" width="80" height="80" />
        <icon src="icon.png" platform="ios" width="120" height="120" />
        <!-- iPhone / iPod Touch -->
        <icon src="icon.png" platform="ios" width="57" height="57" />
        <icon src="icon.png" platform="ios" width="114" height="114" />

        <!-- iPad -->
        <icon src="icon-72.png" platform="ios" width="72" height="72" />
        <icon src="icon-72@2x.png" platform="ios" width="144" height="144" />

        <!-- iPhone Spotlight and Settings Icon -->
        <icon src="icon-small.png" platform="ios" width="29" height="29" />
        <icon src="icon-small@2x.png" platform="ios" width="58" height="58" />

        <!-- iPad Spotlight and Settings Icon -->
        <icon src="icon-50.png" platform="ios" width="50" height="50" />
        <icon src="icon-50@2x.png" platform="ios" width="100" height="100" />


        <splash height="1136" platform="ios" src="res/screen/ios/Default~iphone.png" width="640" />
        <splash height="1334" platform="ios" src="res/screen/ios/Default~iphone.png" width="750" />
        <splash height="2208" platform="ios" src="res/screen/ios/Default~iphone.png" width="1242" />
        <splash height="1242" platform="ios" src="res/screen/ios/Landscape-ipad.png" width="2208" />
        <splash height="1536" platform="ios" src="res/screen/ios/Landscape-ipad.png" width="2048" />
        <splash height="768" platform="ios" src="res/screen/ios/Landscape-ipad.png" width="1024" />
        <splash height="2048" platform="ios" src="res/screen/ios/Portrait-ipad.png" width="1536" />
        <splash height="1024" platform="ios" src="res/screen/ios/Portrait-ipad.png" width="768" />
        <splash height="960" platform="ios" src="res/screen/ios/Default@2x~iphone.png" width="640" />
        <splash height="480" platform="ios" src="res/screen/ios/Default~iphone.png" width="320" />
    </platform>
    <platform name="wp8">
        <icon height="99" platform="wp8" src="img/logo.png" width="99" />
        <icon height="159" platform="wp8" src="img/logo.png" width="159" />
        <splash height="1280" platform="wp8" src="res/screen/wp8/screen-portrait.jpg" width="768" />
    </platform>
    <platform name="windows">
        <icon height="150" platform="windows" src="img/logo.png" width="150" />
        <icon height="30" platform="windows" src="img/logo.png" width="30" />
        <icon height="50" platform="windows" src="img/logo.png" width="50" />
        <splash height="300" platform="windows" src="res/screen/windows/SplashScreen.scale-100.png" width="620" />
        <icon height="120" platform="windows" src="img/logo.png" width="120" />
        <icon height="44" platform="windows" src="img/logo.png" width="44" />
        <icon height="106" platform="windows" src="img/logo.png" width="106" />
        <icon height="70" platform="windows" src="img/logo.png" width="70" />
        <icon height="71" platform="windows" src="img/logo.png" width="71" />
        <icon height="170" platform="windows" src="img/logo.png" width="170" />
        <icon height="360" platform="windows" src="img/logo.png" width="360" />
        <icon height="310" platform="windows" src="img/logo.png" width="310" />
        <icon height="150" platform="windows" src="img/logo.png" width="310" />
        <icon height="360" platform="windows" src="img/logo.png" width="744" />
        <splash height="1920" platform="windows" src="res/screen/windows/SplashScreenPhone.scale-240.png" width="1152" />
    </platform>
    <access origin="*" />
    <access origin="*.googleapis.com" />
    <access origin="*.gstatic.com" />
    <access origin="https://maps.google.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <allow-navigation href="*://*.google.com/*" />
        <allow-intent href="geo:*" />
        <access origin="*.googleapis.com" />
        <access origin="*.gstatic.com" />
        <access origin="https://maps.google.com" />
        <allow-navigation href="*://*.wutz.co.uk/*" />
        <access origin="*.wutz.co.uk" />
        <access origin="*.cdn.rawgit.com" />
        <allow-navigation href="*://*.cdn.rawgit.com/*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <access origin="*.googleapis.com" />
        <access origin="*.gstatic.com" />
        <access origin="https://maps.google.com" />
        <access origin="*.cdn.rawgit.com" />
        <allow-navigation href="*://*.cdn.rawgit.com/*" />
    </platform>
</widget>
